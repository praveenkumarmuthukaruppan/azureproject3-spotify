{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azureproject3-df"
		},
		"Azure_sql_db_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Azure_sql_db'"
		},
		"azure_datalakeGen2_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'azure_datalakeGen2_ls'"
		},
		"Azure_sql_db_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "proserver1.database.windows.net"
		},
		"Azure_sql_db_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "prodatabase"
		},
		"Azure_sql_db_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "praveen"
		},
		"azure_datalakeGen2_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azureproject3storageacc.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azureproject3-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Azure_sql_db_password": {
						"value": "[parameters('Azure_sql_db_password')]"
					},
					"azure_datalakeGen2_ls_accountKey": {
						"value": "[parameters('azure_datalakeGen2_ls_accountKey')]"
					},
					"Azure_sql_db_properties_typeProperties_server": {
						"value": "[parameters('Azure_sql_db_properties_typeProperties_server')]"
					},
					"Azure_sql_db_properties_typeProperties_database": {
						"value": "[parameters('Azure_sql_db_properties_typeProperties_database')]"
					},
					"Azure_sql_db_properties_typeProperties_userName": {
						"value": "[parameters('Azure_sql_db_properties_typeProperties_userName')]"
					},
					"azure_datalakeGen2_ls_properties_typeProperties_url": {
						"value": "[parameters('azure_datalakeGen2_ls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}